@{
    ViewBag.Title = "Home Page";
}
<div class="login">
    <div id="loginToggle">Login</div>
    <form class="popupForm">
        <div>
            <label>Username</label>
            <input />
        </div>
        <div>
            <label>Password</label>
            <input type="password" />
        </div>
        <div>
            <input type="checkbox" />
            <label>Remember Me</label>
        </div>
        <div>
            <input type="submit" value="Login" />
        </div>
    </form>
</div>
<div id="title">
    <h1>Invocations of Greatness!</h1>
</div>
<div id="intro">
    <div id="invocations">

        <img src="~/img/invoker-no-background.png" id="invoker-no-background" />
        <img src="~/img/wex_orb.png" id="wex_orb_left" />
        <img src="~/img/quas-orb.png" id="quas_orb_left" />
        <img src="~/img/exort-orb.png" id="exort_orb_left"/>
        <img src="~/img/wex_orb.png" id="wex_orb_top" />
        <img src="~/img/quas-orb.png" id="quas_orb_top" />
        <img src="~/img/exort-orb.png" id="exort_orb_top"/>
        <img src="~/img/wex_orb.png" id="wex_orb_right" />
        <img src="~/img/quas-orb.png" id="quas_orb_right" />
        <img src="~/img/exort-orb.png" id="exort_orb_right"/>
    </div>
    <form id="hidden-form">
        <input type="text" />
        <input type="text" />
        <input type="text" />
        <ul class="startingMenu">

            <li id="quas">Quas</li>
            <li id="wex">Wex</li>
            <li id="exort">Exort</li>
            <li id="invoke-list-item"><button id="invoke"><span id="background-invoke">Invoke!</span></button></li>

        </ul>
    </form>

</div>
