@{
    ViewBag.Title = "Home Page";
}
<div class="login">
    <div id="loginToggle">Login</div>
    <form class="popupForm">
        <div>
            <label>Username</label>
            <input />
        </div>
        <div>
            <label>Password</label>
            <input type="password" />
        </div>
        <div>
            <input type="checkbox" />
            <label>Remember Me</label>
        </div>
        <div>
            <input type="submit" value="Login" />
        </div>
    </form>
</div>
<div id="title">
    <h1>Invocations of Greatness!</h1>
</div>
<div id="intro">
    <div id="invocations">

        <img src="~/img/invoker-no-background.png" id="invoker-no-background" />
        <img src="~/img/wex_orb.png" id="wex_orb_left" />
    </div>
    <ul class="startingMenu">
        <li id="quas">Quas</li>
        <li id="wex">Wex</li>
        <li id="exort">Exort</li>
        <li id="invoke"><span id="background-invoke">Invoke!</span></li>
    </ul>


</div>
